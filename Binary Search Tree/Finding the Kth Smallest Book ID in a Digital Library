class Node:
    def __init__(self,d):
        self.val=d
        self.left=None
        self.right=None
def bst(root,newval):
    if root is None:
        return Node(newval)
    if newval<root.val:
        root.left=bst(root.left,newval)
    else:
        root.right=bst(root.right,newval)
    return root
def solve(root,temp):
    if root :
        solve(root.left,temp)
        temp.append(root.val)
        solve(root.right,temp)
    return temp
   
    
if __name__=="__main__":
    n=int(input())
    l=list(map(int,input().split()))
    k=int(input())
    root=None
    for newval in l:
        root=bst(root,newval)
    temp=[]
    solve(root,temp)
    print(temp[k-1])
   
    
