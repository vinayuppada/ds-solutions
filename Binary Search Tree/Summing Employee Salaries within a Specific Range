class Node:
    def __init__(self,d):
        self.val=d
        self.left=None
        self.right=None
def bst(root,newval):
    if root is None:
        return Node(newval)
    if newval< root.val:
        root.left=bst(root.left,newval)
    else:
        root.right=bst(root.right,newval)
    return root

def solve(root,a,b,s):
    if root:
        solve(root.left,a,b,s)
        if a<=root.val<=b:
            s.append(root.val)
        solve(root.right,a,b,s)
    
    
    
    
if __name__=="__main__":
    n=int(input())
    l=list(map(int,input().split()))
    a,b=map(int,input().split())
    root=None
    for newval in l:
        root=bst(root,newval)
    s=[]
    solve(root,a,b,s)
    print(sum(s))
    
        
